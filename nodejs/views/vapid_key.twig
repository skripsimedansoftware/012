{% extends 'base.twig' %}

{% block title %}Vapid Key{% endblock %}

{% block content %}
<div class="container">
	<div class="starter-template">
		<h1>Vapid Key</h1>
		<div class="form-group">
			<label>Public Key : </label>
			<code>{{ vapid.publicKey }}</code>
		</div>
		<div class="form-group">
			<label>Private Key : </label>
			<code>{{ vapid.privateKey }}</code>
		</div>
	</div>
</div>
{% endblock %}